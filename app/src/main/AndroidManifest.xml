<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.errlindeleon.androidfinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RestaurantReviewItemsActivity"
            android:label="RestaurantReviews"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.errlindeleon.androidfinal.RestaurantReviewItemsActivity" />
        </activity>
        <activity android:name=".RestaurantItemActivity"
            android:label="RestaurantItems"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.errlindeleon.androidfinal.RestaurantItemActivity" />
        </activity>
        <activity android:name=".ShareFoodActivity"
            android:label="RestaurantItems"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.errlindeleon.androidfinal.ShareFoodActivity" />
        </activity>

    </application>

</manifest>